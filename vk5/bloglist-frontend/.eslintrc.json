{
    "extends": "airbnb",
    "rules": {
      "react/destructuring-assignment": [false, "always"],
      "no-underscore-dangle": "off",
      "react/prop-types": 0,
      "react/jsx-one-expression-per-line": 0,
      "jsx-a11y/click-events-have-key-events": 0
    },
    "globals": {
      "window": true
    }
}
